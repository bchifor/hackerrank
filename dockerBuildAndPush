# Build the container
#DOCKER_REPO=dockers.tf.riotgames.com
DOCKER_REPO=registry.rcluster.io
DOCKER_GNAME=insightstech
IMAGE_NAME=$DOCKER_GNAME/centos_java
IMAGE_VERSION=centos7_java820

#build the repository
docker build -t $IMAGE_NAME:$IMAGE_VERSION .

#docker tag with remote repository name
docker tag $IMAGE_NAME:$IMAGE_VERSION $DOCKER_REPO/$IMAGE_NAME:$IMAGE_VERSION

docker push $DOCKER_REPO/$IMAGE_NAME:$IMAGE_VERSION
